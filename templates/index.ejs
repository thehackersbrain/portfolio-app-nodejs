		<!-- alert start -->
		<% if (fmsg != '') { %>
		<div class="fixed-top" style="margin-top: 3.8rem">
			<div
				class="alert alert-<%= fstatus%> alert-dismissible fade show"
				role="alert"
			>
				<strong><%= fname%>,</strong> <%= fmsg%>
				<button
					type="button"
					class="close"
					data-dismiss="alert"
					aria-label="Close"
				>
					<span aria-hidden="true">&times;</span>
				</button>
			</div>
		</div>
		<% } %>
